class circle
{
   double radius;
   circle(double radius)
   {
      this.radius=radius;
    }
   double area;
   {
    double val = radius*radius*3.14;
    return val;
   }
   double circumfarence;
   {
    double cfc = 2*3.14*radius;
    return cfc;
   }
